{
  "name": "pimcore/skeleton",
  "type": "project",
  "license": "GPL-3.0-or-later",
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "process-timeout": 0,
    "allow-plugins": {
      "cweagans/composer-patches": true,
      "symfony/runtime": true
    }
  },
  "prefer-stable": true,
  "require": {
    "asmblah/php-amqp-compat": "^0.4.13",
    "blackbit_digital_commerce/pimcore-backup": "dev-master",
    "cweagans/composer-patches": "^1.7",
    "pimcore/admin-ui-classic-bundle": "*",
    "pimcore/pimcore": "*",
    "pimcore/platform-version": "^2024.4",
    "pimcore/quill-bundle": "^1.0",
    "presta/sitemap-bundle": "^3.3",
    "symfony/amqp-messenger": "^6.2",
    "symfony/dotenv": "^6.2",
    "symfony/form": "^7.1",
    "symfony/http-client": "^7.1",
    "symfony/runtime": "^6.2",
    "symfony/twig-bundle": "^6.4"
  },
  "require-dev": {
    "codeception/codeception": "^5.0.3",
    "codeception/module-symfony": "^3.1.0"
  },
  "extra": {
    "patches-file": "composer.patches.json"
  },
  "suggest": {
    "pimcore/data-hub": "Universal data interface for GraphQL, CSV and other formats"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "Pimcore\\Model\\DataObject\\": "var/classes/DataObject"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-create-project-cmd": "Pimcore\\Composer::postCreateProject",
    "post-install-cmd": [
      "Pimcore\\Composer::postInstall",
      "@pimcore-scripts"
    ],
    "post-update-cmd": [
      "Pimcore\\Composer::postUpdate",
      "@pimcore-scripts"
    ],
    "pimcore-scripts": [
      "Pimcore\\Composer::clearCache",
      "Pimcore\\Composer::installAssets"
    ]
  }
}
